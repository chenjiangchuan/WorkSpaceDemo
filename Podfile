 # 指定Workspace
workspace 'WorkspaceDemo'
xcodeproj 'KernelProject/KernelProject.xcodeproj'
    
target 'KernelProject' do
 platform :ios, ‘8.0’
 xcodeproj 'KernelProject/KernelProject.xcodeproj'
end

target 'Project1' do
  platform :ios, ‘8.0’
  xcodeproj 'Project1/Project1.xcodeproj'
  pod 'AFNetworking'
end
    
target 'Project2' do
 platform :ios, ‘8.0’
 xcodeproj 'Project2/Project2.xcodeproj'
end
